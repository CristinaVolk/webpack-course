{
  "parser": "babel-eslint",
  "rules": {
    "no-extra-semi": "error",
    "no-unreachable-loop": "error",
    "newline-before-return": "warn",
    "no-unused-vars": "warn"
  },
  "env": {
    "es6": true,
    "amd": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended"]
}
